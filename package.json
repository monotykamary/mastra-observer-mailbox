{
  "name": "mastra-observer-mailbox",
  "version": "0.2.1",
  "description": "A hybrid message-oriented store for passive, event-driven communication between AI agents for Mastra",
  "module": "src/index.ts",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./primitives": {
      "import": "./dist/primitives/index.js",
      "types": "./dist/primitives/index.d.ts"
    }
  },
  "scripts": {
    "build": "bun build src/index.ts --outdir dist --target node && tsc -p tsconfig.build.json",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "typecheck": "tsc --noEmit",
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:major": "standard-version --release-as major",
    "publish:npm": "bun run build && npm publish"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "standard-version": "^9.5.0",
    "typescript": "^5"
  },
  "keywords": [
    "mastra",
    "ai",
    "agents",
    "mailbox",
    "observer",
    "multi-agent",
    "context-injection"
  ],
  "license": "MIT",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ]
}
